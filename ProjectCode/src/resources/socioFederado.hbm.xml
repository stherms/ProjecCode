<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="ProjectCode.modelo.B2_SocioFederado" table = "sociofederado" catalog="estherms">
        <id name="numSocio" column="numSocio"/>
        <property name="nombre" type="string" column="nombre"/>
        <property name="nif" type="string" column="NIF"/>
        <property name="codigoFederacion" type="string" column="codigoFederacion"/>
        <property name="nomFederacion" type="string" column="nomFederacion"/>
    </class>
</hibernate-mapping>
